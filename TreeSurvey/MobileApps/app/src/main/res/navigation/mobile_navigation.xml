<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.testapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_nav_view_surveys"
            app:destination="@id/nav_view_surveys" />
    </fragment>

    <fragment
        android:id="@+id/nav_view_surveys"
        android:name="com.example.testapp.ui.server_status.View_SurveysFragment"
        android:label="@string/menu_view_surveys"
        tools:layout="@layout/fragment_view_surveys" >

        <action
            android:id="@+id/action_nav_view_surveys_to_nav_input_form"
            app:destination="@id/nav_input_form" />
        <action
            android:id="@+id/action_nav_view_surveys_to_nav_edit_data"
            app:destination="@id/nav_edit_data" />
    </fragment>

    <fragment
        android:id="@+id/nav_input_form"
        android:name="com.example.testapp.ui.FormInput.fragment_Form_Input"
        android:label="@string/menu_test"
        tools:layout="@layout/fragment_form_input">

    </fragment>

    <fragment
        android:id="@+id/nav_edit_data"
        android:name="com.example.testapp.ui.viewData.edit_data.EditDataFragment"
        android:label="EditData"
        tools:layout="@layout/fragment_edit_data"
        />
</navigation>